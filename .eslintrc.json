{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error",
    "no-duplicate-functions": "error",
    "no-redeclare": "error",
    "no-var": "error",
    "prefer-const": "warn",
    "no-unused-expressions": "warn"
  },
  "globals": {
    "supabase": "readonly"
  }
}
